<div id="layoutSidenav_content">
    <main>
        <div class="container-fluid">
            <p id="notice"><%= notice %></p>
            <h3 class="my-5">Cadastro</h3>
            <%= link_to 'Editar', edit_unmed_path(@unmed), class:"btn btn-sm btn-success mr-3 my-2" %>
            <%= link_to 'Voltar', unmeds_path, class:"btn btn-sm btn-success my-2" %>
            <p><strong>Abreviação: </strong><%= @unmed.abreviacao %></p>
            <p><strong>Descrição: </strong><%= @unmed.descricao %></p>
        </div>
    </main>
    <%= render 'shared/footer' %>
</div>