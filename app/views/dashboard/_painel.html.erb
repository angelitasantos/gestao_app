<!-- Page Wrapper -->
<div id="wrapper">

    <!-- Sidebar -->
    <%= render 'shared/sidebar' %>
    <!-- End of Sidebar -->

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

        <!-- Main Content -->
        <div id="content">

            <!-- Topbar -->
            <%= render 'shared/header' %>
            <!-- End of Topbar -->

            <!-- Begin Page Content -->
            <%= render 'dashboard/content' %>
            <!-- /.container-fluid -->

        </div>
        <!-- End of Main Content -->

        <!-- Footer -->
        <%= render 'shared/footer' %>
        <!-- End of Footer -->

    </div>
    <!-- End of Content Wrapper -->

</div>
<!-- End of Page Wrapper -->