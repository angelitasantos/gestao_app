<div id="layoutSidenav_content">

    <main>

        <div class="container-fluid">

            <p id="notice"><%= notice %></p>

            <h3 class="my-5">Cadastro</h3>

            <%= link_to 'Editar', edit_typeitem_path(@typeitem), class:"btn btn-sm btn-success mr-3 my-2" %>
            <%= link_to 'Voltar', typeitems_path, class:"btn btn-sm btn-success my-2" %>
            
            <p> <strong>id: </strong><%= @typeitem.id %></p>
            <p> <strong>Descrição: </strong><%= @typeitem.name %></p>

        </div>

    </main>

    <%= render 'shared/footer' %>

</div>