<div id="layoutSidenav_content">

  <main>

    <div class="container-fluid">

      <p id="notice"><%= notice %></p>

      <h3 class="my-5">Cadastro</h3>

      <p><strong>Descricao:</strong><%= @category.descricao %></p>
      <p><strong>User:</strong><%= @category.user_id %></p>

      <%= link_to 'Editar', edit_category_path(@category), class:"btn btn-secondary mr-3" %>
      <%= link_to 'Voltar', categories_path, class:"btn btn-secondary" %>
        
    </div>

  </main>

  <%= render 'shared/footer' %>

</div>